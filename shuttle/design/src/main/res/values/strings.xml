    <!-- region Permissions -->
    <string name="permissions_title">Permissions</string>
    <string name="permissions_skip_permissions_action">Skip permissions</string>

    <string name="permissions_accessibility_name">Accessibility service</string>
    <string name="permissions_accessibility_description">Shuttle needs access to the Accessibility Service, in order to improve the suggestions.\nBy enabling it you consent Shuttle to detect app launching and store it to the local database</string>
    <string name="permissions_accessibility_action">Enable accessibility service</string>
    <string name="permissions_accessibility_granted_description">Accessibility service is enabled</string>
    <string name="permissions_accessibility_not_granted_description">Accessibility service is not enabled</string>
    <string name="permissions_accessibility_dialog_disclosure">Shuttle needs access to the Accessibility Service, in order to improve the suggestions.\nBy enabling it you consent Shuttle to detect app launching and store it to the local database\n\nIn the accessibility management screen (or in \"Installed services\" in some devices) find \'Shuttle\' and enable it. Shortcut is not necessary</string>
    <string name="permissions_accessibility_dialog_confirm_action">Confirm</string>
    <string name="permissions_accessibility_dialog_cancel_action">Cancel</string>

    <string name="permissions_location_action">Allow location permissions</string>
    <string name="permissions_location_coarse_name">Location permissions</string>
    <string name="permissions_location_coarse_description">Shuttle needs access to Location, in order to show suggested apps, based on your current location.\nBy allowing the permission you consent Shuttle to store a Geo Hash of your current position and store it to the local database</string>
    <string name="permissions_location_coarse_granted_description">Approximate location permission is granted</string>
    <string name="permissions_location_coarse_not_granted_description">Approximate location permission is not granted</string>
    <string name="permissions_location_fine_name">Precise location permissions</string>
    <string name="permissions_location_fine_description">Shuttle needs access to Precise Location, in order to create a Geo Hash to store to the local database</string>
    <string name="permissions_location_fine_granted_description">Precise location permission is granted</string>
    <string name="permissions_location_fine_not_granted_description">Precise location permission is not granted</string>
    <string name="permissions_location_background_name">Background location permissions</string>
    <string name="permissions_location_background_description">Shuttle needs access to Background Location, in order to create a Geo Hash to store to the local database every time an app has been opened. Additionally, Shuttle will use the current location in the Widget, in order to provide suggestions based on the current location</string>
    <string name="permissions_location_background_granted_description">Background location permission is granted</string>
    <string name="permissions_location_background_not_granted_description">Background location permission is not granted</string>
    <!-- endregion -->

